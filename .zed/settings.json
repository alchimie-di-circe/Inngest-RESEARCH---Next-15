{
  "theme": "One Dark Pro",
  "terminal": {
    "shell": "bash",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "languages": {
    "TypeScript": {
      "tab_size": 2,
      "soft_tabs": true,
      "format_on_save": "on",
      "formatter": "prettier"
    }
  },
  "extensions": [
    "prettier",
    "eslint",
    "typescript-tools"
  ]
}
